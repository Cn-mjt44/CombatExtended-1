<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<!-- 18% sharp armor and 18% blunt armor stats balanced accordingly and adjusted for CE -->

	<Operation Class="PatchOperationReplace">
		<xpath>Defs/HediffDef[defName="RH_HiverSoldierHead_Hediff"]/stages/li/statOffsets</xpath>
		<value>
			<statOffsets>
				<ArmorRating_Blunt>10</ArmorRating_Blunt>
				<ArmorRating_Sharp>3</ArmorRating_Sharp>
			</statOffsets>
		</value>
	</Operation>

</Patch>